#include <iostream>

using namespace std;

int main()
{
    int save;
    cin>>save;
    
    if(save>5000)
    {
        if(save>8000)
        {
        cout<<"buy big lv bag";
        }
        
        else
        {
            cout<<"small lv bag";
        }
        
    }
    else if(save>2000)
    {
        cout<<"buy plastic bag";
    }
    else 
    {
        cout<<"dont buy anything";
    }

    return 0;
}
